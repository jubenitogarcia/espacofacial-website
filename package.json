{
  "name": "espacofacial-open",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "next lint",
    "smoke": "node scripts/smoke.mjs",
    "cf:security": "node scripts/cloudflare-sync-security.mjs",
    "tail": "wrangler tail espacofacial-site --format pretty --sampling-rate 0.25",
    "tail:json": "wrangler tail espacofacial-site --format json",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "deploy:esfa": "wrangler deploy -c workers/esfa-redirector/wrangler.toml"
  },
  "dependencies": {
    "@svg-maps/brazil": "^2.0.0",
    "next": "14.2.35",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^1.16.3",
    "@types/node": "25.2.2",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "eslint": "10.0.0",
    "eslint-config-next": "16.1.6",
    "typescript": "5.9.3",
    "wrangler": "^4.63.0"
  },
  "overrides": {
    "@next/eslint-plugin-next": {
      "glob": "10.5.0"
    },
    "wrangler": {
      "undici": "7.18.2"
    }
  }
}
